<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="tint_color" format="color" />
    <declare-styleable name="NineKeyButton">
        <attr name="search_view_id" format="reference"/>
        <attr name="keywords" format="string"/>
        <attr name="number" format="string" />
        <attr name="drawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="NineKeyDialpad">
        <attr name="divider_color" format="color"/>
        <attr name="show_keyword" format="boolean"/>
        <attr name="tint_color"/>
        <attr name="tint_color2" format="color"/>
        <attr name="colorful" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="DeletableEditText">
        <attr name="clean_icon" format="reference"/>
        <attr name="tint_color"/>
    </declare-styleable>
    <declare-styleable name="PasswordTextView">
        <attr name="show_clear_icon" format="boolean"/>
        <attr name="dot_color" format="color"/>
        <attr name="dot_width" format="dimension"/>
        <attr name="password_length" format="integer"/>
        <attr name="text_size" format="dimension"/>
        <attr name="show_password" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ColorfulAnim">
        <attr name="speed_factor" format="float" />
    </declare-styleable>
</resources>